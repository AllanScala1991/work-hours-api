generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id String @id @default(uuid())
  name String
  email String @unique
  phone String
  address String
  username String @unique
  password String
  position String
  sector String
  authorization String
  secretQuestion String
  secretAnswer String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}